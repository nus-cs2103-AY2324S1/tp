<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="introViewing" text="You are viewing a person:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="nameDisplayed" styleClass="cell_big_label" text="\$first">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin></Label>
            <FlowPane fx:id="tagsDisplayed" />
            <HBox VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="phoneLabel" text="Phone: " />
                  <Label fx:id="phoneDisplayed" text="phone" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="emailLabel" text="Email: " />
                  <Label fx:id="emailDisplayed" text="email" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="addressLabel" text="Address: " />
                  <Label fx:id="addressDisplayed" text="address" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
