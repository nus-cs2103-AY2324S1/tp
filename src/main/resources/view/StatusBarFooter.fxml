          xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets top="5" right="10" bottom="5" left="10" />
  </padding>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
  </columnConstraints>
  <Label fx:id="saveLocationStatus" />
</GridPane>
