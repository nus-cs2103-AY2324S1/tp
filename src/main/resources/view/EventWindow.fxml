<?import javafx.scene.layout.StackPane?>
<fx:root resizable="false" title="Events" minWidth="600" minHeight="400" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/help_icon.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
                <URL value="@Extensions.css" />
                <URL value="@Form.css" />
            </stylesheets>

            <StackPane fx:id="eventListPanelPlaceholder" styleClass="pane-with-border" minWidth="150" prefWidth="150">
                <padding>
                    <Insets top="10" right="10" bottom="10" left="10" />
                </padding>
            </StackPane>
        </Scene>
    </scene>
</fx:root>
