@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyAddressBook" as ReadOnlyAddressBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class AddressBook
Class ModelManager
Class UserPrefs

Class UniqueApplicantList
Class UniqueInterviewList
Class Applicant
Class Interview
Class Address
Class Email
Class Name
Class Phone
Class Tag
Class Jobrole
Class Rating
Class String
Class Time

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

AddressBook .up.|> ReadOnlyAddressBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyAddressBook
ModelManager -left-> "1" AddressBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

AddressBook *--> "1" UniqueApplicantList
AddressBook *--> "1" UniqueInterviewList
UniqueApplicantList --> "~* all" Applicant
UniqueInterviewList --> "~* all" Interview
Applicant *--> Name
Applicant *--> Phone
Applicant *--> Email
Applicant *--> Address
Applicant *--> "*" Tag
Interview --> "1" Applicant
Interview *--> "jobRole" String
Interview *--> Rating
Interview *--> "startTime" Time
Interview *--> "endTime" Time

Applicant -[hidden]up--> I
UniqueApplicantList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager --> "~* filtered" Applicant
ModelManager --> "~* filtered" Interview
@enduml
