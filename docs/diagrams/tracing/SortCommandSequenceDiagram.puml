@startuml
!include ../style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
Participant ":SortCommand" as sc LOGIC_COLOR
Participant ":CommandResult" as cr LOGIC_COLOR
end box
box Model MODEL_COLOR_T1
Participant "model:Model" as m MODEL_COLOR
end box

[-> sc ++: execute()
sc -> m ++: sortJobs()
deactivate m
create cr
sc -> cr ++: CommandResult()
cr --> sc --: result
sc -->[ --: result
@enduml
